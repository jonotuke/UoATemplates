\item {\bf Revision of some mathematical concepts}

\emph{This question may be handwritten. Remember to scan and attach to rest of assignment.}

\begin{enumerate}
	\item For the following calculate $y$.

<<include=FALSE>>=
x <- 5; a <- 2; b <- 3
@

\begin{enumerate}
	\item $y = \Sexpr{a} + \Sexpr{b}x, x = \Sexpr{x}$.
	
	\partmark[1]
	
	\begin{solutions}
		$ y = \Sexpr{a} + \Sexpr{b}x = \Sexpr{a} + \Sexpr{b}\times \Sexpr{x} = \Sexpr{a} + \Sexpr{b*x} = \Sexpr{a+b*x}\ms$
	\end{solutions}

<<include=FALSE>>=
b <- 3; x <- 3
@

	\item $y = \Sexpr{b}x, x = \Sexpr{x}$
	
	\partmark[1]
	
	\begin{solutions}
		$y = \Sexpr{b} x = \Sexpr{b} \times \Sexpr{x}  = \Sexpr{b*x} \ms$
	\end{solutions}

<<include=FALSE>>=
a <- 1; b <- 4; x <- 10
@

	\item $y = a + bx, a = \Sexpr{a} , b = \Sexpr{b} , x = \Sexpr{x} $
	
	\partmark[1]
	
	\begin{solutions}
		$y = a + bx = \Sexpr{a}  + \Sexpr{b}  \times \Sexpr{x}  
		= \Sexpr{a}  + \Sexpr{b*x}  = \Sexpr{a+b*x} \ms$
	\end{solutions}

	
<<include=FALSE>>=
a <- 1; x <- 9; b <- 4
@
	\item $y = \frac{x-a}{b}, x = \Sexpr{x} , a = \Sexpr{a} , b = \Sexpr{b} $
	
	\partmark[1]
	
	\begin{solutions}
		$y = \frac{x-a}{b} = \frac{\Sexpr{x} - \Sexpr{a} }{\Sexpr{b} } = 
		\frac{\Sexpr{x-a} }{\Sexpr{b} } = \Sexpr{(x-a)/b} \ms$
	\end{solutions}
\end{enumerate}

